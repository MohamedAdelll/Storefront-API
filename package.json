{
  "name": "store",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "set NODE_ENV=dev && npx tsc && nodemon ./src/server.ts",
    "test": "set NODE_ENV=test && db-migrate -e test up && npx tsc && jasmine & db-migrate -e test reset",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess nodemon ./dist/server.js",
    "build": "npx tsc",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write src/**/**/*.ts",
    "migration:run": "db-migrate up",
    "migration:down": "db-migrate down"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "-": "^0.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^14.18.22",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.20.0",
    "eslint-config-google": "^0.14.0",
    "jasmine": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.16",
    "npm": "^8.13.2",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  }
}
